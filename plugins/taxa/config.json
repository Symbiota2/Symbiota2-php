{
  "name": "taxa",
  "title": "Taxa Plugin",
  "source": "local",
  "version": "0.0.1",
  "description": "Adds the Taxa module to manage taxonomic data.",
  "api_namespace": "Taxa",
  "ui_filename": "taxa.umd.js",
  "ui_module_name": "TaxaModule",
  "component_hooks": [
    {
      "outlet": "global-permission-available",
      "index": 2,
      "provider": "taxonomy-editor-available-permission"
    },
    {
      "outlet": "global-permission-available",
      "index": 3,
      "provider": "taxon-profile-editor-available-permission"
    },
    {
      "outlet": "global-permission-current",
      "index": 2,
      "provider": "taxonomy-editor-current-permission"
    },
    {
      "outlet": "global-permission-current",
      "index": 3,
      "provider": "taxon-profile-editor-current-permission"
    }
  ]
}
