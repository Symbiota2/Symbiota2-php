{
  "name": "symbiota2",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_checklist": "ng build checklist",
    "npm_pack_checklist": "cd dist/checklist && npm pack",
    "move_checklist": "run-script-os",
    "move_checklist:win32": "move .\\dist\\checklist\\bundles\\checklist.umd.js .\\src\\ui\\assets\\js\\plugins\\",
    "move_checklist:darwin:linux": "mv ./dist/checklist/bundles/checklist.umd.js ./src/ui/assets/js/plugins/",
    "package_checklist": "npm run build_checklist && npm run npm_pack_checklist && npm run move_checklist",
    "build_collection": "ng build collection",
    "npm_pack_collection": "cd dist/collection && npm pack",
    "move_collection": "run-script-os",
    "move_collection:win32": "move .\\dist\\collection\\bundles\\collection.umd.js .\\src\\ui\\assets\\js\\plugins\\",
    "move_collection:darwin:linux": "mv ./dist/collection/bundles/collection.umd.js ./src/ui/assets/js/plugins/",
    "package_collection": "npm run build_collection && npm run npm_pack_collection && npm run move_collection",
    "build_occurrence": "ng build occurrence",
    "npm_pack_occurrence": "cd dist/occurrence && npm pack",
    "move_occurrence": "run-script-os",
    "move_occurrence:win32": "move .\\dist\\occurrence\\bundles\\occurrence.umd.js .\\src\\ui\\assets\\js\\plugins\\",
    "move_occurrence:darwin:linux": "mv ./dist/occurrence/bundles/occurrence.umd.js ./src/ui/assets/js/plugins/",
    "package_occurrence": "npm run build_occurrence && npm run npm_pack_occurrence && npm run move_occurrence",
    "build_occurrence-search": "ng build occurrence-search",
    "npm_pack_occurrence-search": "cd dist/occurrence-search && npm pack",
    "move_occurrence-search": "run-script-os",
    "move_occurrence-search:win32": "move .\\dist\\occurrence-search\\bundles\\occurrence-search.umd.js .\\src\\ui\\assets\\js\\plugins\\",
    "move_occurrence-search:darwin:linux": "mv ./dist/occurrence-search/bundles/occurrence-search.umd.js ./src/ui/assets/js/plugins/",
    "package_occurrence-search": "npm run build_occurrence-search && npm run npm_pack_occurrence-search && npm run move_occurrence-search",
    "build_taxa": "ng build taxa",
    "npm_pack_taxa": "cd dist/taxa && npm pack",
    "move_taxa": "run-script-os",
    "move_taxa:win32": "move .\\dist\\taxa\\bundles\\taxa.umd.js .\\src\\ui\\assets\\js\\plugins\\",
    "move_taxa:darwin:linux": "mv ./dist/taxa/bundles/taxa.umd.js ./src/ui/assets/js/plugins/",
    "package_taxa": "npm run build_taxa && npm run npm_pack_taxa && npm run move_taxa",
    "build_symbiota-spatial": "ng build symbiota-spatial",
    "npm_pack_symbiota-spatial": "cd dist/symbiota-spatial && npm pack",
    "install_symbiota-spatial": "npm install dist/symbiota-spatial/symbiota-spatial-0.0.1.tgz",
    "package_symbiota-spatial": "npm run build_symbiota-spatial && npm run npm_pack_symbiota-spatial && npm run install_symbiota-spatial",
    "build_symbiota-auth": "ng build symbiota-auth",
    "npm_pack_symbiota-auth": "cd dist/symbiota-auth && npm pack",
    "install_symbiota-auth": "npm install dist/symbiota-auth/symbiota-auth-0.0.1.tgz",
    "package_symbiota-auth": "npm run build_symbiota-auth && npm run npm_pack_symbiota-auth && npm run install_symbiota-auth",
    "build_symbiota-shared": "ng build symbiota-shared",
    "npm_pack_symbiota-shared": "cd dist/symbiota-shared && npm pack",
    "install_symbiota-shared": "npm install dist/symbiota-shared/symbiota-shared-0.0.1.tgz",
    "package_symbiota-shared": "npm run build_symbiota-shared && npm run npm_pack_symbiota-shared && npm run install_symbiota-shared",
    "build_symbiota-plugin": "ng build symbiota-plugin",
    "npm_pack_symbiota-plugin": "cd dist/symbiota-plugin && npm pack",
    "install_symbiota-plugin": "npm install dist/symbiota-plugin/symbiota-plugin-0.0.1.tgz",
    "package_symbiota-plugin": "npm run build_symbiota-plugin && npm run npm_pack_symbiota-plugin && npm run install_symbiota-plugin",
    "build_symbiota-plugin-loader": "ng build symbiota-plugin-loader",
    "npm_pack_symbiota-plugin-loader": "cd dist/symbiota-plugin-loader && npm pack",
    "install_symbiota-plugin-loader": "npm install dist/symbiota-plugin-loader/symbiota-plugin-loader-0.0.1.tgz",
    "package_symbiota-plugin-loader": "npm run build_symbiota-plugin-loader && npm run npm_pack_symbiota-plugin-loader && npm run install_symbiota-plugin-loader",
    "package_all_plugins": "npm run package_checklist && npm run package_collection && npm run package_occurrence && npm run package_occurrence-search && npm run package_taxa && npm run package_symbiota-spatial && npm run package_symbiota-auth && npm run package_symbiota-shared && npm run package_symbiota-plugin && npm run package_symbiota-plugin-loader",
    "package_optional_plugins": "npm run package_checklist && npm run package_collection && npm run package_occurrence && npm run package_occurrence-search && npm run package_taxa",
    "package_core_plugins": "npm run package_symbiota-shared && npm run package_symbiota-auth && npm run package_symbiota-spatial && npm run package_symbiota-plugin && npm run package_symbiota-plugin-loader",
    "postinstall" : "npm run package_core_plugins && npm run package_optional_plugins"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
