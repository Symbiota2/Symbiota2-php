    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@ngrx/store": "^8.0.0",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@turf/turf": "^5.1.6",
    "buffer": "^5.2.1",
    "core-js": "^3.1.3",
    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "font-awesome": "4.7.0",
    "hammerjs": "^2.0.8",
    "html2canvas": "^1.0.0-rc.3",
    "jszip": "^3.2.1",
    "npm-check-updates": "^3.1.10",
    "ol": "^5.3.3",
    "rxjs": "^6.5.2",
    "shpjs": "^3.4.3",
