    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^11.13.0",
    "codelyzer": "^5.0.1",
    "cross-env": "^5.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "prettier": "^1.17.1",
    "protractor": "^5.4.2",
    "run-script-os": "^1.0.7",
    "ts-loader": "^6.0.1",
    "ts-node": "~8.0.3",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.3",
    "tslint": "~5.15.0",
    "typescript": "~3.4.5",
    "webpack-cli": "^3.3.3"
  }
}
