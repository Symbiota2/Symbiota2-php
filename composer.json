{
    "name": "Symbiota2/Symbiota2",
    "description": "Symbiota2",
    "keywords": ["Symbiota", "Symbiota2"],
    "license": "GPL-2.0-only",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-cors": "^0.11.2",
        "beberlei/DoctrineExtensions": "^1.1",
        "creof/doctrine2-spatial": "^1.2",
        "fideloper/proxy": "^4.0",
        "floriansemm/solr-bundle": "^1.6",
        "flugger/laravel-responder": "^3.0",
        "gedmo/doctrine-extensions": "^2.4",
        "laravel-doctrine/extensions": "^1.0",
        "laravel-doctrine/migrations": "^1.2",
        "laravel-doctrine/orm": "^1.4",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "^1.0",
        "phpoffice/phpword": "^0.15"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "core/database/seeds",
            "core/database/factories"
        ],
        "psr-4": {
            "App\\": "core/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "core/tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('core/config/database.php') || copy('core/config/database_example.php', 'core/config/database.php');\"",
            "@php -r \"file_exists('core/.env') || copy('core/.env.example', 'core/.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan config:cache"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
